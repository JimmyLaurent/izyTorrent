{
  "name": "izytorrent",
  "version": "1.0.0",
  "description": "A uTorrent mobile web app to grab and manage torrents.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "server-dev": "nodemon --debug server/app.js",
    "client-dev": "node npm-client.js start",
    "build": "node npm-client.js run build",
    "client-install": "node npm-client.js install",
    "server": "node server/app.js",
    "start": "npm run server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "http-auth": "^3.1.3",
    "lodash": "^4.17.4",
    "sanitize-filename": "^1.6.1",
    "torrent-search-api": "^1.0.12",
    "utorrent-api": "^0.1.5",
    "uuid": "^3.0.1"
  }
}
